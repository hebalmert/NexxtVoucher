@model NexxtVoucher.Models.TicketTime

@{
    ViewBag.Title = @Resources.Resource.ViewDelete;
}

    <h2>@Resources.Resource.ViewDelete</h2>

    <h3>@Resources.Resource.MsgDelete</h3>
    <div>
        <h4>@Resources.Resource.TicketTime_Titulo</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <dl class="dl-horizontal">

        <dt>
            @Html.DisplayNameFor(model => model.Orden)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Orden)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.TiempoTicket)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TiempoTicket)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.ScriptTicketConsumo)
        </dt>

        <dd>
            @Html.EditorFor(model => model.ScriptTicketConsumo, new { htmlAttributes = new { @readonly = "True", @class = "form-control", @style = "height: 100px;max-width:700px" } })
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.ScriptTicket)
        </dt>

        <dd>
            @Html.EditorFor(model => model.ScriptTicket, new { htmlAttributes = new { @readonly = "True", @class = "form-control", @style = "height: 750px;max-width:700px" } })
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.IsActive)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IsActive)
        </dd>

    </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

    <div class="form-actions no-color">
        <button type="button" class="btn btn-success btn-sm" onclick="location.href = '@Url.Action("Index", new  {})'">
            <i class="fa fa-backspace"></i>
            @Resources.Resource.ButtonBack
        </button>

        <button type="submit" class="btn btn-danger btn-sm">
            <i class="fa fa-trash"></i>
            @Resources.Resource.ButtonDelete
        </button>
    </div>
        }
    </div>
