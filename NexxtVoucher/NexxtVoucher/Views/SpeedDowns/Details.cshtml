@model NexxtVoucher.Models.SpeedDown

@{
    ViewBag.Title = @Resources.Resource.ViewDetail;
}

    <h2>@Resources.Resource.ViewDetail</h2>

<div>
    <h4>@Resources.Resource.SpeedDown_Titulo</h4>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

<dl class="dl-horizontal">
    <dt>
        @Html.DisplayNameFor(model => model.Orden)
    </dt>

    <dd>
        @Html.DisplayFor(model => model.Orden)
    </dd>

    <dt>
        @Html.DisplayNameFor(model => model.VelocidadDown)
    </dt>

    <dd>
        @Html.DisplayFor(model => model.VelocidadDown)
    </dd>

</dl>
</div>
<p>
    <button type="button" class="btn btn-success btn-sm" onclick="location.href = '@Url.Action("Index", new  {})'">
        <i class="fa fa-backspace"></i>
        @Resources.Resource.ButtonBack
    </button>

    <button type="submit" class="btn btn-warning btn-sm" onclick="location.href = '@Url.Action("Edit", new { id = Model.SpeedDownId })'">
        <i class="fa fa-pencil-alt"></i>
        @Resources.Resource.ButtonEdit
    </button>
</p>
